{
    "name": "exonchat",
    "private": true,
    "version": "0.0.1",
    "description": "ExonChat",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "TZ=UTC nest start --watch",
        "start:dev:db": "rm -rf dist/ && nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/src/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
        "seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
        "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
        "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
        "drop:seed": "npm run schema:drop; npm run schema:sync; ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
        "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'",
        "db:model": "npx prisma generate",
        "db:drop": "npx prisma db push --preview-feature --force-reset --accept-data-loss",
        "db:seed": "npx prisma db seed --preview-feature",
        "db:drop:seed": "npm run db:drop; npm run db:seed",
        "studio": "npx prisma studio -b none"
    },
    "dependencies": {
        "@nestjs-modules/mailer": "^1.6.0",
        "@nestjs/common": "^7.6.6",
        "@nestjs/config": "^0.6.3",
        "@nestjs/core": "^7.5.1",
        "@nestjs/jwt": "^7.2.0",
        "@nestjs/mapped-types": "^0.3.0",
        "@nestjs/passport": "^7.1.5",
        "@nestjs/platform-express": "^7.5.1",
        "@nestjs/platform-socket.io": "^7.6.6",
        "@nestjs/throttler": "^1.1.1",
        "@nestjs/websockets": "^7.6.6",
        "@prisma/client": "^2.25.0",
        "bcrypt": "^5.0.1",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "cookie-parser": "^1.4.5",
        "faker": "^4.1.0",
        "helmet": "^4.6.0",
        "lodash": "^4.17.20",
        "moment": "^2.29.1",
        "nodemailer": "^6.6.1",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pg": "^8.5.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.6.3",
        "socket.io-redis": "^5.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.5.1",
        "@nestjs/schematics": "^7.1.3",
        "@nestjs/testing": "^7.6.14",
        "@types/bcrypt": "^5.0.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/express": "^4.17.8",
        "@types/faker": "^5.1.6",
        "@types/jest": "^26.0.15",
        "@types/multer": "^1.4.5",
        "@types/node": "^14.14.35",
        "@types/passport-jwt": "^3.0.5",
        "@types/passport-local": "^1.0.33",
        "@types/redis": "^2.8.28",
        "@types/socket.io": "^2.1.13",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^4.6.1",
        "@typescript-eslint/parser": "^4.6.1",
        "eslint": "^7.12.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "^26.6.3",
        "prettier": "^2.1.2",
        "prisma": "^2.25.0",
        "supertest": "^6.0.0",
        "ts-jest": "^26.4.3",
        "ts-loader": "^8.0.8",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.2.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
